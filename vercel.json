{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "questions.html",
            "use": "@vercel/static"
        },
        {
            "src": "img/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/questions.html",
            "dest": "/questions.html"
        },
        {
            "src": "/img/(.*)",
            "dest": "/img/$1"
        },
        {
            "src": "/index.html",
            "dest": "/index.html"
        },
        {
            "src": "^/$",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}